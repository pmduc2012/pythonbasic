__author__ = 'MinhDuc'
class Animals():
    def __init__(self, name):
        self.name = name

    def animalSay(self):
        pass

class Tiger(Animals):
    def animalSay(self):
        return 'grwww'

class Elephant(Animals):
    def animalSay(self):
        return 'trumpet'

class Cat(Animals):
    def animalSay(self):
        return 'meo meo'

class Dog(Animals):
    def animalSay(self):
        return 'gau gau'

class Rabbit(Animals):
    def animalSay(self):
        return 'oe oe'

tiger = Tiger('tiger')
elephant = Elephant('elephant')
cat = Cat('cat')
dog = Dog('dog')
rabbit = Rabbit('rabbit')


animals = [tiger, elephant, cat, dog, rabbit]

for animal in animals:
    print(type(animal).__name__, 'say', animal.animalSay() )

